{
  "name": "uncodeference.website",
  "description": "The website for the un{code}ference.io unconference, based on gatsby-starter-typescript-plus.",
  "version": "1.1.0",
  "private": false,
  "author": "Bernhard Mayr <bernhard@mayr.com>, Juan Luis Vicci <juan@gronda.eu>",
  "keywords": [
    "gatsby",
    "uncodeference",
    "unconference"
  ],
  "license": "MIT",
  "main": "n/a",
  "scripts": {
    "build": "gatsby build",
    "clean": "rimraf public",
    "deploy": "gatsby build --prefix-paths && gh-pages -d public",
    "dev": "gatsby develop",
    "format": "prettier --write \"src/**/*.{ts,tsx,md}\"",
    "format:quick": "pretty-quick --staged",
    "lint": "tslint \"src/**/*.{ts,tsx}\" -p tsconfig.json",
    "lint:fix": "tslint \"src/**/*.{ts,tsx}\" -p tsconfig.json --fix",
    "start": "npm run dev",
    "test": "npm run type-check && npm run lint",
    "type-check": "tsc"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "dependencies": {
    "@emotion/core": "^10.0.5",
    "@emotion/styled": "^10.0.5",
    "babel-plugin-styled-components": "^1.7.1",
    "classnames": "^2.2.6",
    "gatsby": "^2.3.14",
    "gatsby-image": "^2.0.5",
    "gatsby-plugin-canonical-urls": "^2.0.5",
    "gatsby-plugin-emotion": "^4.0.1",
    "gatsby-plugin-manifest": "^2.0.22",
    "gatsby-plugin-react-helmet": "^3.0.0",
    "gatsby-plugin-sharp": "^2.0.32",
    "gatsby-plugin-typescript": "^2.0.0",
    "gatsby-remark-copy-linked-files": "^2.0.5",
    "gatsby-remark-images": "^2.0.1",
    "gatsby-remark-prismjs": "^3.0.0",
    "gatsby-remark-responsive-iframe": "^2.0.5",
    "gatsby-remark-smartypants": "^2.0.5",
    "gatsby-source-filesystem": "^2.0.28",
    "gatsby-transformer-json": "^2.1.1",
    "gatsby-transformer-remark": "^2.1.3",
    "gatsby-transformer-sharp": "^2.1.1",
    "modern-normalize": "^0.5.0",
    "normalize.css": "^8.0.0",
    "polished": "^2.0.3",
    "prism-themes": "^1.0.1",
    "prismjs": "^1.15.0",
    "react": "^16.5.2",
    "react-anchor-link-smooth-scroll": "^1.0.12",
    "react-dom": "^16.5.2",
    "react-helmet": "^5.2.0",
    "typescript": "^3.0.3"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.6",
    "@types/node": "^10.10.1",
    "@types/react": "^16.4.14",
    "@types/react-dom": "^16.0.7",
    "@types/react-helmet": "^5.0.7",
    "gh-pages": "^2.0.0",
    "husky": "^1.3.1",
    "prettier": "^1.14.2",
    "pretty-quick": "^1.10.0",
    "rimraf": "^2.6.2",
    "tslint": "^5.11.0",
    "tslint-config-blvd": "^1.2.1",
    "tslint-config-prettier": "^1.15.0",
    "tslint-plugin-prettier": "^2.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run format:quick && npm run lint:fix",
      "pre-push": "npm test"
    }
  }
}
